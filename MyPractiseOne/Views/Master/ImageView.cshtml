
@{
    ViewBag.Title = "ImageView";
}
<br /><br /><br /><br /><br /><br /><br />
<div class="col-md-9">

    <label id="imgget">

    </label>
</div>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $(document).ready()
    {
        GetImage();
    }
    function GetImage()
    {
        $.ajax({
            url: GlobalURL + "Master/GetImage",
            type: "POST",
            data: { },
            success: function (data)
            {
    
                $("#imgget").html(data.Grid);
            },
            error: function () {
                alert("Image  Not Load");
            }
        })
    }

</script>
